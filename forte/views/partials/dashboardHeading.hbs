<div class="dashboard-header">
    <h1>{{user.companyId.companyName}}</h1>
    <div class="buttons-dashboard-header">
        {{#ifvalue user.role equals="Administrator"}}
            <a href="/{{user.companyId.url}}/settings"><button>Settings</button></a>
        {{/ifvalue}}
        <form action="/{{user.companyId.url}}/sign-out" method="POST">
            <button>Logout</button>
        </form>    
    </div>
</div>
<h5>{{user.role}} Dashboard</h5>